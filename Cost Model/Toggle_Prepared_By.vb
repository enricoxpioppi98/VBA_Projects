Sub Toggle_Prepared_By()

    Improve_Execution.ScreenUpdating_And_Calculation

    'Unprotect ---------------------------------------------------------------------------------------------------------------------------------------

    '------------------------------------------------------------------------------------------------------------------------------------------------

    For Each ws In Active.Workbook.Worksheets


    Next ws


    'Restore Execution -----------------------------------------------------------------------------------------------------------------------------
        Application.ScreenUpdating = True
        Application.Calculation = xlCalculationAutomatic
    '------------------------------------------------------------------------------------------------------------------------------------------------


End Sub